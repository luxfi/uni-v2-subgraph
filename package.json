{
  "name": "@luxfi/uniswap-v2-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/luxfi/uni-v2-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "lint": "eslint . --ext .ts --fix",
    "build": "run-s codegen && graph build",
    "buildonly": "graph build",
    "codegen": "graph codegen --output-dir src/types/",
    "create": "graph create luxfi/uniswap --node https://graph.lux.network",
    "deploy": "RUST_LOG=debug graph deploy luxfi/uniswap --ipfs https://ipfs.lux.network --node https://graph.lux.network",
    "watch-local": "graph deploy graphprotocol/Uniswap2 --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.64.1",
    "@graphprotocol/graph-ts": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "@uniswap/eslint-config": "^1.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
